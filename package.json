{
  "name": "react-uni-skeleton",
  "version": "0.0.0",
  "description": "Skeleton for React Projects",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint *.js \"**/*.js\"",
    "lint:fix": "eslint *.js \"**/*.js\" --fix",
    "start": "echo \"Error: no start specified. Please run start:dev / start:prod / start:prod:ssr\" && exit 0",
    "start:dev": "node entrypoint/server/dev.js",
    "start:prod": "node entrypoint/server/prod.js",
    "start:prod:ssr": "node entrypoint/server/prod.ssr.js",
    "build": "webpack --config config/webpack.config.prod.js",
    "build:serverless": "webpack --config config/webpack.config.prod.serverless.js",
    "build:ssr:client": "webpack --config config/webpack.config.prod.ssr.client.js",
    "build:ssr:server": "webpack --config config/webpack.config.prod.ssr.server.js",
    "build:ssr": "npm run build:ssr:client && npm run build:ssr:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChDeinert/react-uni-skeleton.git"
  },
  "author": "Christian Deinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChDeinert/react-uni-skeleton/issues"
  },
  "homepage": "https://github.com/ChDeinert/react-uni-skeleton#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.0.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "react-hot-loader": "^3.1.3",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
