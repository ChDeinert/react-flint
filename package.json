{
  "name": "react-tinder",
  "version": "0.0.0",
  "description": "Boilerplate for starting React Projects",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js \"**/*.js\"",
    "lint:fix": "eslint *.js \"**/*.js\" --fix",
    "start": "echo \"Error: no start specified. Please run start:dev / start:prod / start:prod:ssr\" && exit 0",
    "start:dev": "node entrypoint/server/dev.js",
    "start:prod": "node entrypoint/server/prod.js",
    "start:prod:ssr": "node entrypoint/server/prod.ssr.js",
    "build": "webpack --config config/webpack.config.prod.js",
    "build:serverless": "webpack --config config/webpack.config.prod.serverless.js",
    "build:ssr:client": "webpack --config config/webpack.config.prod.ssr.client.js",
    "build:ssr:server": "webpack --config config/webpack.config.prod.ssr.server.js",
    "build:ssr": "npm run build:ssr:client && npm run build:ssr:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChDeinert/react-tinder.git"
  },
  "author": "Christian Deinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChDeinert/react-tinder/issues"
  },
  "homepage": "https://github.com/ChDeinert/react-tinder#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.0.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "react-hot-loader": "^3.1.3",
    "style-loader": "^0.20.3",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
