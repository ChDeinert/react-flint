{
  "name": "react-flint",
  "version": "0.0.0",
  "description": "Boilerplate to help ignite React Projects",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js \"**/*.js\"",
    "lint:fix": "eslint *.js \"**/*.js\" --fix",
    "start": "echo \"Error: no start specified. Please run start:dev / start:prod / start:prod:ssr\" && exit 0",
    "start:dev": "node src/entrypoint/server/dev.js",
    "start:dev:ssr": "node src/entrypoint/server/dev.ssr.js",
    "start:prod": "node src/entrypoint/server/prod.js",
    "start:prod:ssr": "node src/entrypoint/server/prod.ssr.js",
    "build": "webpack --config config/webpack.config.prod.js",
    "build:serverless": "webpack --config config/webpack.config.prod.serverless.js",
    "build:ssr": "webpack --config config/webpack.config.prod.ssr.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChDeinert/react-flint.git"
  },
  "author": "Christian Deinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChDeinert/react-flint/issues"
  },
  "homepage": "https://github.com/ChDeinert/react-flint#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@testing-library/react": "^9.3.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "bootstrap": "^4.3.1",
    "chokidar": "^3.2.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-hot-loader": "^4.12.15",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "object-assign": "^4.1.1",
    "promise": "^8.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "whatwg-fetch": "^3.0.0"
  }
}
