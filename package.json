{
  "name": "react-flint",
  "version": "0.0.0",
  "description": "Boilerplate to help ignite React Projects",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js \"**/*.js\"",
    "lint:fix": "eslint *.js \"**/*.js\" --fix",
    "start": "echo \"Error: no start specified. Please run start:dev / start:prod / start:prod:ssr\" && exit 0",
    "start:dev": "node entrypoint/server/dev.js",
    "start:prod": "node entrypoint/server/prod.js",
    "start:prod:ssr": "node entrypoint/server/prod.ssr.js",
    "build": "webpack --config config/webpack.config.prod.js",
    "build:serverless": "webpack --config config/webpack.config.prod.serverless.js",
    "build:ssr:client": "webpack --config config/webpack.config.prod.ssr.client.js",
    "build:ssr:server": "webpack --config config/webpack.config.prod.ssr.server.js",
    "build:ssr": "npm run build:ssr:client && npm run build:ssr:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChDeinert/react-flint.git"
  },
  "author": "Christian Deinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChDeinert/react-flint/issues"
  },
  "homepage": "https://github.com/ChDeinert/react-flint#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.2.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "whatwg-fetch": "^2.0.4"
  }
}
