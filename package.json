{
  "name": "react-flint",
  "version": "0.0.0",
  "description": "Boilerplate to help ignite React Projects",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js \"**/*.js\"",
    "lint:fix": "eslint *.js \"**/*.js\" --fix",
    "start": "echo \"Error: no start specified. Please run start:dev / start:prod / start:prod:ssr\" && exit 0",
    "start:dev": "node entrypoint/server/dev.js",
    "start:prod": "node entrypoint/server/prod.js",
    "start:prod:ssr": "node entrypoint/server/prod.ssr.js",
    "build": "webpack --config config/webpack.config.prod.js",
    "build:serverless": "webpack --config config/webpack.config.prod.serverless.js",
    "build:ssr:client": "webpack --config config/webpack.config.prod.ssr.client.js",
    "build:ssr:server": "webpack --config config/webpack.config.prod.ssr.server.js",
    "build:ssr": "npm run build:ssr:client && npm run build:ssr:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChDeinert/react-flint.git"
  },
  "author": "Christian Deinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChDeinert/react-flint/issues"
  },
  "homepage": "https://github.com/ChDeinert/react-flint#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0-beta.55",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "bootstrap": "^4.2.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.6.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "object-assign": "^4.1.1",
    "promise": "^8.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "whatwg-fetch": "^3.0.0"
  }
}
