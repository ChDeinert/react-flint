{
  "presets": ["@babel/env", "@babel/react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-regenerator",
    "@babel/plugin-transform-runtime",
    [
      "module-resolver", {
        "alias": {
          "Components": "./src/App/components",
          "AppSource": "./src/App",
          "Public": "./public",
          "ServerSource": "./src/server"
        }
      }
    ]
  ]
}
